{
  "name": "parallax",
  "productName": "Parallax",
  "version": "1.0.0",
  "description": "Advanced Google Dorking Toolkit - AI-powered OSINT and security research platform with Gemini AI integration",
  "author": "Parallax Team",
  "license": "SEE LICENSE FILE",
  "private": true,
  "main": "electron/main.js",
  "keywords": [
    "electron",
    "google-dorking",
    "osint",
    "security-research",
    "gemini-ai",
    "cybersecurity",
    "reconnaissance",
    "dork-generator",
    "shodan",
    "censys",
    "desktop-app"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "setup": "node setup.js",
    "dev": "vite",
    "build": "vite build",
    "build:vercel": "vite build",
    "build:electron": "cross-env ELECTRON=true vite build",
    "preview": "vite preview",
    "build:prod": "NODE_ENV=production vite build",
    "clean": "rm -rf dist node_modules out",
    "start": "npm run build && npm run preview",
    "package": "node scripts/package.js",
    "deploy": "node scripts/deploy.js",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && cross-env VITE_DEV_SERVER_URL=http://localhost:3000 electron .\"",
    "electron:build": "npm run build:electron && electron-forge package",
    "electron:start": "electron-forge start",
    "electron:package": "electron-forge package",
    "electron:make": "electron-forge make",
    "electron:make:win": "electron-forge make --platform=win32",
    "electron:make:mac": "electron-forge make --platform=darwin",
    "electron:make:linux": "electron-forge make --platform=linux",
    "electron:publish": "electron-forge publish"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "electron-store": "^8.1.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-dmg": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-wix": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-fuses": "^7.2.0",
    "@electron-forge/publisher-github": "^7.2.0",
    "@electron/fuses": "1.8.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "archiver": "^7.0.1",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^28.1.3",
    "electron-builder": "^26.7.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-electron": "^0.29.0",
    "wait-on": "^9.0.3"
  }
}
